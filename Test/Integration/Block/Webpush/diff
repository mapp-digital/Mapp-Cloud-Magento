+            ->with($this->eventMock, $nestedLevel)
+            ->willReturn([
+                'some_field' => 1,
+                'data_model' => [
+                    'entity_id' => 1,
+                    'value' => 2,
+                    'value2' => 3
+                ]
+            ]);
